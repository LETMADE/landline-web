{
  "name": "landline",
  "version": "0.1.0",
  "description": "ASM chat, without the ASM (hosted chat for anyone)",
  "main": "dist/landline.js",
  "scripts": {
    "dev": "NODE_ENV=development webpack-dev-server --hot --colors --config webpack.config.dev.js",
    "prepublish": "npm test && webpack",
    "start": "npm run dev",
    "test": "jest"
  },
  "repository": "git@github.com:asm-products/landline-web.git",
  "keywords": [
    "chat",
    "assembly",
    "asm"
  ],
  "author": "chuck@assembly.com",
  "license": "AGPL",
  "engines": "0.10.x",
  "dependencies": {
    "basscss": "^4.2.4",
    "express": "^4.11.2",
    "firebase": "^2.2.0",
    "flux": "^2.0.1",
    "immutable": "^3.6.2",
    "nprogress": "^0.1.6",
    "react": "^0.12.2",
    "wegen": "^0.1.0",
    "zepto": "0.0.4"
  },
  "devDependencies": {
    "babel-core": "^4.1.1",
    "babel-loader": "^4.0.0",
    "css-loader": "^0.9.1",
    "jest-cli": "^0.3.0",
    "jsx-loader": "^0.12.2",
    "nodemon": "^1.3.7",
    "react-hot-loader": "^1.1.5",
    "react-tools": "^0.12.2",
    "style-loader": "^0.8.3",
    "webpack": "^1.5.3",
    "webpack-dev-server": "^1.7.0"
  },
  "jest": {
    "collectCoverage": true,
    "scriptPreprocessor": "<rootDir>/preprocessor.js",
    "setupEnvScriptFile": "<rootDir>/jest_env.js",
    "testPathDirs": [
      "<rootDir>/src"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules"
    ]
  }
}
